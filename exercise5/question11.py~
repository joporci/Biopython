#11 manages list of names
name=raw_input("Enter name: ")
First=name
Last=name
Shortest=name
Longest=name

while name != '':
	if name < First:
		First=name
	if name > Last:
		Last=name
	if len(name) < len(Shortest):
		Shortest=name
	if len(name) > len(Longest):
		Longest=name
	name=raw_input("Enter name: ")

print 'This is the First name alphabetically:', First
print 'This is the Last name alphabetically: ', Last
print 'This is the Shortest name: ', Shortest
print 'This is the Longest name: ', Longest
		

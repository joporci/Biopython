grades = [('Maths', 'D'), ('Comp Sci', 'B'), ('English', 'C'), ('French', 'A'), ('Science', 'B'), ('History', 'E')]
string = ''
print "Subjects","\t","Grade"
for i in grades:
	name = i[0]
	grade = i[1]
	print name,"\t",grade




